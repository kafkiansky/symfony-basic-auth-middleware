{
    "name": "kafkiansky/symfony-basic-auth-middleware",
    "description": "Basic Auth PSR-15 middleware for Symfony framework.",
    "type": "library",
    "require": {
        "php": "^8.0",
        "psr/http-server-middleware": "^1.0",
        "nyholm/psr7": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Kafkiansky\\SymfonyMiddleware\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kafkiansky\\SymfonyMiddleware\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "v.zanfir",
            "email": "vadimzanfir@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "scripts": {
        "test": "./vendor/bin/phpunit --do-not-cache-result"
    }
}
